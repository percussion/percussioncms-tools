<xsd:schema xmlns="http://www.percussion.com/schema/property"
            xmlns:xsd="http://www.w3.org/2001/XMLSchema"
            targetNamespace="http://www.percussion.com/schema/config"
            elementFormDefault="qualified"
            attributeFormDefault="unqualified">

