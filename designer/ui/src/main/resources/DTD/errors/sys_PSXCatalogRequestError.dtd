
<?xml version='1.0' encoding='UTF-8'?>

<!--
   This file contains the DTD that specifies the error message that is 
   used to report an error encountered during a catalog request.

   Attributes:
   category - the catalog request category
   type - the catalog request type
-->

<!ELEMENT PSXCatalogRequestError (description)>
<!ATTLIST PSXCatalogRequestError
   category CDATA #IMPLIED
   type CDATA #IMPLIED
>

<!--
   Detailed description of the error.
-->
<!ELEMENT description (#PCDATA)>

